{
  "name": "@tinyland-inc/tinyvectors",
  "version": "0.1.0",
  "description": "Animated vector blob backgrounds with physics simulation for Svelte 5",
  "type": "module",
  "license": "Zlib AND LicenseRef-Tinyland-Proprietary",
  "author": {
    "name": "Jess Sullivan",
    "email": "jess@sulliwood.org"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tinyland/tinyland.dev",
    "directory": "packages/tinyvectors"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "animation",
    "svg",
    "physics",
    "blobs",
    "vectors",
    "skeleton-ui",
    "accessibility",
    "pride",
    "lgbtq"
  ],
  "svelte": "./dist/svelte/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./motion": {
      "types": "./dist/motion/index.d.ts",
      "default": "./dist/motion/index.js"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "default": "./dist/themes/index.js"
    },
    "./themes/css": "./dist/themes/vector-colors.css",
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "default": "./dist/svelte/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --config vite.dev.config.ts",
    "dev:watch": "vite build --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pbt": "vitest run --testNamePattern='INVARIANT'",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "fast-check": "^3.23.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.0"
  }
}
